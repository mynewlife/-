.swiper{
  position: absolute;
  top: 150rpx;
  border-radius: 40rpx;
}
.swiper swiper {
  width: 700rpx;
  height: 240rpx;
  margin: 0 auto;
}
.wx-swiper-dots.wx-swiper-dots-horizontal {
  position: absolute;
  z-index: 999;
  height: 6rpx;
}
.wx-swiper-dot {
  width: 30rpx;
  display: inline-flex;
  height: 4rpx;
  justify-content: space-between;
  border-radius: 0 !important;
  width: 20rpx !important;
  height: 8rpx !important;
  background-color: rgb(233, 230, 230);
}
.wx-swiper-dot::before {
  content: "";
  flex-grow: 1;
  background: rgb(218, 217, 217) !important;
  border-radius: 0 !important;
}
.wx-swiper-dot-active::before {
  background: #fdfdfd !important;
  border-radius: 0 !important;
}
.wx-swiper-dot.wx-swiper-dot-active {
  border-radius: 0 !important;
  width: 30rpx !important;
  height: 8rpx !important;
  background-color: #fff;
}
.swiper swiper-item{
  border-radius: 40rpx;
}
.swiper swiper image {
  width: 100%;
}
.cate {
