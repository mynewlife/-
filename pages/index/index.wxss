.floor .floorgroup .floorTitle image {
  width: 100%;
}
.floor .floorgroup .floor_List {
  overflow: hidden;
  clear: both;
}
.floor .floorgroup .floor_List navigator {
  float: left;
  width: 33.33%;
  box-sizing: border-box;
}
.floor .floorgroup .floor_List navigator:nth-last-child(-n+4) {
  height: 27.70215517vw;
  border-left: 10rpx solid #fff;
}
.floor .floorgroup .floor_List navigator:nth-child(2),
.floor .floorgroup .floor_List navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor .floorgroup .floor_List navigator image {
  width: 100%;
  height: 100%;
