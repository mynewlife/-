page {
  background: #f7f7f7;
  color: #151515;
  font-size: 28rpx;
  -webkit-text-size-adjust: 100% !important;
  padding-bottom: env(safe-area-inset-bottom);
}
.header {
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.header .hd_bar_wrap {
  height: 100rpx;
  position: relative;
  z-index: 10;
  background: #fff;
  line-height: 28rpx;
  font-size: 28rpx;
}
.header .hd_bar_wrap .hd_bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  margin: 0 auto;
  width: 100%;
  z-index: 10;
  padding: 20rpx;
  background: #fff;
}
.header .hd_bar_wrap .hd_bar .hd_search_frm {
  -webkit-flex: 1;
  -webkit-box-flex: 1;
  flex: 1;
  position: relative;
  height: 60rpx;
}
.header .hd_bar_wrap .hd_bar .hd_search_frm .hd_virtual_input {
  height: 56rpx;
  padding: 0 60rpx;
  border-radius: 30rpx;
  border: 1px solid #eee;
  line-height: 54rpx;
  color: #999;
}
.header .hd_bar_wrap .hd_bar .hd_search_frm:before {
  position: absolute;
  content: "";
  top: 16rpx;
  left: 20rpx;
  width: 30rpx;
  height: 30rpx;
  background: url(https://img12.360buyimg.com/img/s30x30_jfs/t1/157927/26/3929/1022/60056b7eEd7c7de90/9613501c60b5c16e.png) no-repeat;
  background-size: 30rpx;
}
.header .my_nav {
  position: relative;
  z-index: 8;
  height: 88rpx;
}
.header .my_nav .my_nav_inner {
  position: relative;
  padding-right: 86rpx;
  background-color: #fff;
}
.header .my_nav .my_nav_inner .my_nav_list {
  height: 88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  white-space: nowrap;
}
.header .my_nav .my_nav_inner .my_nav_list .list_item {
  -webkit-flex: 1;
  -webkit-box-flex: 1;
  flex: 1;
  text-align: center;
  display: inline-block;
}
.header .my_nav .my_nav_inner .my_nav_list .list_item .item {
  position: relative;
  display: inline-block;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  color: #666;
}
.header .my_nav .my_nav_inner .my_nav_list .list_item .cur {
  color: #f2270c;
  border-bottom: 6rpx solid #f2270c;
}
.header .my_nav .my_nav_inner .icon-xiala1 {
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
  width: 86rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: #e5e5e5;
}
.order_all {
  margin-top: 220rpx;
}
.order_all .order .order_box {
  display: block;
  margin: 30rpx 20rpx;
  padding: 20rpx;
  background: #fff;
  border-radius: 20rpx;
  box-shadow: 0 4rpx 16rpx 0 rgba(0, 0, 0, 0.05);
}
.order_all .order .order_box .order_head {
  line-height: 48rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.order_all .order .order_box .order_head .shop_icon {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  -webkit-align-self: center;
  align-self: center;
  overflow: hidden;
  position: relative;
}
.order_all .order .order_box .order_head .shop_icon image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  border: 0;
  vertical-align: top;
}
.order_all .order .order_box .order_head .shop_title {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  overflow: hidden;
}
.order_all .order .order_box .order_head .shop_title .title {
  display: inline-block;
  vertical-align: middle;
  font-size: 32rpx;
  color: #333;
  position: relative;
  max-width: 90%;
  padding-right: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.order_all .order .order_box .order_head .shop_title .title::after {
  content: "";
  position: absolute;
  display: inline-block;
  top: 50%;
  margin-top: -6rpx;
  right: 6rpx;
  width: 12rpx;
  height: 12rpx;
  border-top: 4rpx solid #ccc;
  border-left: 4rpx solid #ccc;
  background: none;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
}
.order_all .order .order_box .order_head .order_state {
  color: #999;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  display: inline-block;
  vertical-align: middle;
}
.order_all .order .order_box .order_head .cur {
  color: #f2270c;
}
.order_all .order .order_box .order_head .order_del {
  display: inline-block;
  width: 70rpx;
  height: 42rpx;
  position: relative;
  vertical-align: middle;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
}
.order_all .order .order_box .order_head .order_del::before {
  content: "";
  position: absolute;
  width: 1px;
  height: 30rpx;
  background-color: #e3e3e3;
  top: 8rpx;
  left: 20rpx;
}
.order_all .order .order_box .order_head .order_del::after {
  content: "";
  position: absolute;
  top: 6rpx;
  right: 0;
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat 50%;
  background-size: 30rpx;
}
.order_all .order .order_box .order_item {
  display: block;
}
.order_all .order .order_box .order_item .oi_content {
  margin-top: 16rpx;
  display: flex;
  overflow: hidden;
}
.order_all .order .order_box .order_item .oi_content .cover {
  width: 150rpx;
  height: 150rpx;
  margin-right: 20rpx;
  border-radius: 12rpx;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  position: relative;
}
.order_all .order .order_box .order_item .oi_content .cover .img {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
  display: inline-block;
  overflow: hidden;
  position: relative;
}
.order_all .order .order_box .order_item .oi_content .cover .img image {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  border: 0;
  vertical-align: top;
}
.order_all .order .order_box .order_item .oi_content .content {
  color: #333;
  -webkit-align-self: center;
  align-self: center;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.order_all .order .order_box .order_item .oi_content .content .desc {
  color: #333;
  line-height: 42rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.order_all .order .order_box .order_item .oi_content .oi_num {
  margin-left: 16rpx;
  -webkit-align-self: center;
  align-self: center;
  color: #999;
}
.order_all .order .order_box .order_item .order_total_bar {
  margin-top: 10rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
  justify-content: flex-end;
}
.order_all .order .order_box .order_item .order_total_bar .payment {
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 42rpx;
}
.order_all .order .order_box .order_item .order_total_bar .payment .price {
  font-size: 32rpx;
  color: #151515;
  vertical-align: bottom;
}
.order_all .order .order_box .order_btn {
  padding-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: row-reverse;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: reverse;
  flex-direction: row-reverse;
  font-size: 28rpx;
  color: #333;
  line-height: 60rpx;
  position: relative;
}
.order_all .order .order_box .order_btn .oh_btn {
  text-align: center;
  width: 172rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 30rpx;
  margin-left: 16rpx;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  position: relative;
  color: #f2270c;
  background: #fff;
  border: 1px solid #f2270c;
  box-shadow: 0 6rpx 12rpx 0 rgba(242, 39, 12, 0.1);
}
.order_all .order .order_box .order_btn .btn {
  text-align: center;
  width: 172rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 30rpx;
  margin-left: 16rpx;
  -webkit-flex-shrink: 0;
  flex-shrink: 0;
  position: relative;
  color: #fff;
  background: #f2270c;
  border: 1px solid #f2270c;
  box-shadow: 0 6rpx 12rpx 0 rgba(242, 39, 12, 0.1);
}
.order_all .order .order_box .order_btn .time {
  font-size: 24rpx;
}
.order_all .order .order_box .order_btn::before {
  content: "";
  height: 0;
  display: block;
  border-top: 1px solid #e5e5e5;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  position: absolute;
  left: 20rpx;
  right: 0;
  top: 0;
}
